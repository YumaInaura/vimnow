# For Mac OS
if [ $(uname) = 'Darwin' ]; then
  export PATH=$HOME/vimnow/mac/gnu:$PATH

  function date() { $HOME/vimnow/mac/gnu/gdate $@; }
  function stat() { $HOME/vimnow/mac/gnu/gstat $@; } 
  function find() { $HOME/vimnow/mac/gnu/gfind $@; }
  function sed() { $HOME/vimnow/mac/gnu/gsed $@; }
fi


